{
  "appId": "com.subhajit.zentunes",
  "appName": "zentunes",
  "webDir": "dist",
  "bundledWebRuntime": false,
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 3000,
      "backgroundColor": "#31511e",
      "androidSplashResourceName": "splash",
      "showSpinner": false
    },
    "Filesystem": {
      "iosDefaultLocation": "Documents",
      "androidDefaultLocation": "ExternalStorage"
    }
  }
}
